{

  "manifest_version": 2,
  "name": "TabControl",
  "version": "0.2",

  "description": "Change Tab open/close behavior",
  
  "options_ui": {
    "page": "html/options.html",
    "browser_style": true
  },
  
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ],

  
  "background": {
    "scripts": ["js/background-script.js"]
  }
}